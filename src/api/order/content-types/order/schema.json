{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "branch": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::branch.branch"
    },
    "products": {
      "type": "component",
      "repeatable": true,
      "component": "components.inventory-item-with-length"
    },
    "note": {
      "type": "richtext"
    },
    "export": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::warehouse-export.warehouse-export",
      "inversedBy": "order"
    },
    "order_statuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-status.order-status",
      "mappedBy": "order"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "cod",
        "online",
        "pos"
      ]
    },
    "order_invoice": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order-invoice.order-invoice",
      "mappedBy": "order"
    },
    "receive_address": {
      "type": "component",
      "repeatable": false,
      "component": "components.receive-address"
    },
    "isDebt": {
      "type": "boolean",
      "default": false
    },
    "delivery_method": {
      "type": "component",
      "repeatable": false,
      "component": "components.delivery-method"
    },
    "order_payment_vnpay": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order-payment-vnpay.order-payment-vnpay",
      "inversedBy": "orders"
    },
    "discount_value": {
      "type": "biginteger",
      "default": "0"
    }
  }
}
