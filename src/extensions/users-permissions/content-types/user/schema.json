{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "branches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::branch.branch",
      "mappedBy": "manager"
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "components.address"
    },
    "phone": {
      "type": "string",
      "minLength": 9,
      "maxLength": 15
    },
    "name": {
      "type": "component",
      "repeatable": false,
      "component": "components.name"
    },
    "shift": {
      "type": "component",
      "repeatable": false,
      "component": "components.user-shift"
    },
    "salary_per_shift": {
      "type": "integer"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    },
    "refunds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::refund.refund",
      "mappedBy": "customer"
    },
    "voucher_uses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::voucher-use.voucher-use",
      "mappedBy": "customer"
    },
    "receive_address": {
      "type": "component",
      "repeatable": true,
      "component": "components.receive-address"
    }
  }
}
